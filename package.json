{
  "name": "@hiddenlayerai/hiddenlayer-sdk",
  "version": "0.4.1-alpha",
  "description": "Official HiddenLayer TypeScript SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiddenlayerai/hiddenlayer-sdk-typescript.git"
  },
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "clean": "rm -rf dist && rm -rf node_modules",
    "examples": "ts-node examples/model_scanning.ts",
    "lint": "eslint",
    "test": "jest --forceExit"
  },
  "author": "HiddenLayer",
  "license": "Apache-2.0",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.0",
    "@azure/storage-blob": "^12.23.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.5.0",
    "globals": "^15.6.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.6.0"
  }
}
